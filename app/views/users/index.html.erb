
<h1>Listing articles</h1>
<%= link_to 'New User', new_user_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Mobile</th>
    <th>Password</th>
    <th>DateofBirth</th>
    <th>Date</th>
    <th>Age</th>
    <th></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.mobile %></td>
      <td><%= user.password %></td>
      <td><%= user.dob %></td>
      <td><%= user.date %></td>
      <td><%= user.age %></td>
      <td><%= link_to 'Show',user_path(user) %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user_path(user),
      method: :delete,
      data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>